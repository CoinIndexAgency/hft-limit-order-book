{
  "name": "hft-limit-order-book",
  "version": "0.0.6",
  "description": "Node.js Lmit Order Book for high-frequency trading (HFT).",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "addscope": "node tools/packagejson name @fasenderos/hft-limit-order-book",
    "bench": "node benchmarks/benchmark_lob.js",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "lint": "lint-staged",
    "package": "npm run build && npm pack",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest --no-cache --runInBand",
    "test:dev": "jest --no-cache --runInBand --watch",
    "test:cov": "jest --coverage --no-cache --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "exchange",
    "hft",
    "hft-trading",
    "limit-order-book",
    "matching-algorithm",
    "matching-engine",
    "nodejs",
    "orderbook",
    "order-book",
    "typescript",
    "trading"
  ],
  "author": "Andrea Fassina <andrea.fassina82@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/fasenderos/hft-limit-order-book",
  "repository": {
    "type": "git",
    "url": "git@github.com:fasenderos/hft-limit-order-book.git"
  },
  "bugs": {
    "url": "https://github.com/fasenderos/hft-limit-order-book/issues"
  },
  "dependencies": {
    "denque": "2.1.0",
    "functional-red-black-tree": "1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/functional-red-black-tree": "1.0.1",
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "fastify": "^4.3.0",
    "gaussian": "^1.2.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "nanobench": "2.1.1",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
